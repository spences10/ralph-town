{
	"name": "@ralph-town/core",
	"version": "0.0.0",
	"type": "module",
	"description": "Core orchestrator and runtimes for Ralph Loop",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./orchestrator": {
			"types": "./dist/orchestrator.d.ts",
			"import": "./dist/orchestrator.js"
		},
		"./runtime": {
			"types": "./dist/runtime/index.d.ts",
			"import": "./dist/runtime/index.js"
		}
	},
	"scripts": {
		"build": "tsc && cp src/sandbox-agent.ts dist/",
		"dev": "tsc --watch",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@anthropic-ai/claude-agent-sdk": "^0.2.19",
		"@daytonaio/sdk": "^0.134.0",
		"@langfuse/client": "^4.5.1",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/sdk-node": "^0.211.0",
		"langfuse": "^3.38.6"
	},
	"peerDependencies": {
		"dotenv": "^17.0.0"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"typescript": "^5.9.3"
	}
}
